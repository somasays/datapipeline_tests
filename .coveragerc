[run]
branch = True
source =
    datapipeline_tests

[paths]
source =
    src/datapipeline_tests
    **/site-packages/datapipeline_tests

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
title = datapipeline_tests Coverage Test Report
directory = docs/source/_static/coverage
